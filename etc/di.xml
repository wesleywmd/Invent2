<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="invent_acl" xsi:type="object">Wesleywmd\Invent\Console\Command\InventAclCommand</item>
                <item name="invent_block" xsi:type="object">Wesleywmd\Invent\Console\Command\InventBlockCommand</item>
                <item name="invent_command" xsi:type="object">Wesleywmd\Invent\Console\Command\InventCommandCommand</item>
                <item name="invent_config" xsi:type="object">Wesleywmd\Invent\Console\Command\InventConfigCommand</item>
                <item name="invent_controller" xsi:type="object">Wesleywmd\Invent\Console\Command\InventControllerCommand</item>
                <item name="invent_cron" xsi:type="object">Wesleywmd\Invent\Console\Command\InventCronCommand</item>
                <item name="invent_logger" xsi:type="object">Wesleywmd\Invent\Console\Command\InventLoggerCommand</item>
                <item name="invent_menu" xsi:type="object">Wesleywmd\Invent\Console\Command\InventMenuCommand</item>
                <item name="invent_model" xsi:type="object">Wesleywmd\Invent\Console\Command\InventModelCommand</item>
                <item name="invent_module" xsi:type="object">Wesleywmd\Invent\Console\Command\InventModuleCommand</item>
                <item name="invent_preference" xsi:type="object">Wesleywmd\Invent\Console\Command\InventPreferenceCommand</item>
                <!--<item name="invent_admin_crud" xsi:type="object">Wesleywmd\Invent\Console\Command\InventAdminCrudCommand</item>-->
            </argument>
        </arguments>
    </type>

    <virtualType name="aclComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Acl\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventAclCommand">
        <arguments>
            <argument name="component" xsi:type="object">aclComponent</argument>
        </arguments>
    </type>

    <virtualType name="blockComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="phpRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Block\PhpRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventBlockCommand">
        <arguments>
            <argument name="component" xsi:type="object">blockComponent</argument>
        </arguments>
    </type>

    <virtualType name="commandComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="phpRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Command\PhpRenderer</item>
            </argument>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Command\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventCommandCommand">
        <arguments>
            <argument name="component" xsi:type="object">commandComponent</argument>
        </arguments>
    </type>

    <virtualType name="configComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Config\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventConfigCommand">
        <arguments>
            <argument name="component" xsi:type="object">configComponent</argument>
            <argument name="acl" xsi:type="object">aclComponent</argument>
        </arguments>
    </type>

    <virtualType name="controllerComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="phpRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Controller\PhpRenderer</item>
            </argument>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Controller\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventControllerCommand">
        <arguments>
            <argument name="component" xsi:type="object">controllerComponent</argument>
        </arguments>
    </type>

    <virtualType name="cronComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="phpRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Cron\PhpRenderer</item>
            </argument>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Cron\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventCronCommand">
        <arguments>
            <argument name="component" xsi:type="object">cronComponent</argument>
        </arguments>
    </type>

    <virtualType name="loggerComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="phpRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Logger\PhpRenderer</item>
                <item name="handlerRenderer" xsi:type="object">Wesleywmd\Invent\Model\Logger\HandlerPhpRenderer</item>
            </argument>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Logger\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventLoggerCommand">
        <arguments>
            <argument name="component" xsi:type="object">loggerComponent</argument>
        </arguments>
    </type>

    <virtualType name="menuComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Menu\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventMenuCommand">
        <arguments>
            <argument name="component" xsi:type="object">menuComponent</argument>
            <argument name="acl" xsi:type="object">aclComponent</argument>
        </arguments>
    </type>

    <virtualType name="modelComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="phpRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Model\PhpRenderer</item>
                <item name="collectionRenderer" xsi:type="object">Wesleywmd\Invent\Model\Model\CollectionPhpRenderer</item>
                <item name="interfaceRenderer" xsi:type="object">Wesleywmd\Invent\Model\Model\InterfacePhpRenderer</item>
                <item name="repositoryInterfaceRenderer" xsi:type="object">Wesleywmd\Invent\Model\Model\RepositoryInterfacePhpRenderer</item>
                <item name="repositoryRenderer" xsi:type="object">Wesleywmd\Invent\Model\Model\RepositoryPhpRenderer</item>
                <item name="resourceRenderer" xsi:type="object">Wesleywmd\Invent\Model\Model\ResourcePhpRenderer</item>
                <item name="searchResultsInterfaceRenderer" xsi:type="object">Wesleywmd\Invent\Model\Model\SearchResultsInterfacePhpRenderer</item>
            </argument>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Model\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventModelCommand">
        <arguments>
            <argument name="component" xsi:type="object">modelComponent</argument>
            <argument name="preference" xsi:type="object">preferenceComponent</argument>
        </arguments>
    </type>

    <virtualType name="moduleComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="phpRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Module\PhpRenderer</item>
            </argument>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Module\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventModuleCommand">
        <arguments>
            <argument name="component" xsi:type="object">moduleComponent</argument>
            <argument name="block" xsi:type="object">blockComponent</argument>
            <argument name="controller" xsi:type="object">controllerComponent</argument>
            <argument name="command" xsi:type="object">commandComponent</argument>
            <argument name="cron" xsi:type="object">cronComponent</argument>
            <argument name="model" xsi:type="object">modelComponent</argument>
        </arguments>
    </type>

    <virtualType name="preferenceComponent" type="Wesleywmd\Invent\Model\Component\BaseComponent">
        <arguments>
            <argument name="xmlRenderers" xsi:type="array">
                <item name="baseRenderer" xsi:type="object">Wesleywmd\Invent\Model\Preference\XmlRenderer</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Wesleywmd\Invent\Console\Command\InventPreferenceCommand">
        <arguments>
            <argument name="component" xsi:type="object">preferenceComponent</argument>
        </arguments>
    </type>
</config>